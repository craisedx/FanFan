@{
    ViewData["Title"] = "Home Page";
}
<div class="container">
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img  class="d-block w-100" src='https://res.cloudinary.com/fanfancloud/image/upload/v1625907833/2cbb6e47e000cc12cd7ca136774413e8_e9hxsb.jpg'alt="Первый слайд" />
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="https://res.cloudinary.com/fanfancloud/image/upload/v1625908530/maxresdefault_echnes.jpg" alt="Второй слайд">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="https://res.cloudinary.com/fanfancloud/image/upload/v1625908987/57514274079adc7d66da882cb907b7e9_k5g3nu.jpg" alt="Третий слайд">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <br class="breadcrumb">
    <div id="last_add">
        <h1 class="display-5">Последние добавления</h1>
        <br class="breadcrumb">
        <div class="row ">
            @foreach (var item in ViewBag.NewFive)
            {
                <div class="col-6 col-md-3 bg-secondary shadow p-3 mb-5 bg-white rounded m-2">
                    <a href="/Home/FanFiction/@item.Id">
                        <img src=@item.Picture alt="..." class="img-thumbnail">
                    </a>
                    <a href="/Home/FanFiction/@item.Id">
                        <h3 class="display-5">@item.Name</h3>
                    </a>
                    <h5 class="display-5">@item.AppUser.UserName</h5>
                    <h5>
                        <span class="badge badge-pill badge-success float-right">Новое</span>
                    </h5>
                </div>
            }
        </div>
    </div>
    <br class="breadcrumb">
    <div id="fandom">
        <h1 class="display-5">Фендомы</h1>
        <br class="breadcrumb">
        <div class="row">
            @foreach (var item in ViewBag.Fandoms)
            {
                <div class="col-6 col-md-3 bg-secondary shadow p-3 mb-5 bg-white rounded m-2">
                    <h3 class="display-5">@item.Name</h3>
                </div>
            }
        </div>
    </div>
</div>